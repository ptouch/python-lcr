####### Compile

usb.o: usb.cpp usb.h \
		hidapi-master/hidapi/hidapi.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o usb.o usb.cpp

API.o: API.cpp API.h \
		usb.h \
		Common.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o API.o API.cpp

BMPParser.o: BMPParser.cpp Common.h \
		Error.h \
		Config.h \
		BMPParser.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o BMPParser.o BMPParser.cpp

